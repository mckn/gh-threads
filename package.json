{
  "name": "gh-notifications",
  "version": "1.0.0",
  "description": "Automatically mark GitHub notifications as done when they don't require action",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "cli": "ts-node src/cli.ts",
    "cli:build": "npm run build && node dist/cli.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "bin": {
    "gh-notifications": "./dist/cli.js"
  },
  "keywords": [
    "github",
    "notifications",
    "automation",
    "productivity"
  ],
  "author": "Marcus Andersson",
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@types/commander": "^2.12.5",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.19.19",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
